{
  "kind": "collectionType",
  "collectionName": "home_office_requests",
  "info": {
    "singularName": "home-office-request",
    "pluralName": "home-office-requests",
    "displayName": "Home Office Request"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "datetime",
      "required": true
    },
    "endDate": {
      "type": "datetime",
      "required": true
    },
    "approvalStatus": {
      "type": "enumeration",
      "required": true,
      "default": "Pending",
      "enum": [
        "Pending",
        "Approved",
        "Rejected"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "home_office_requests"
    }
  }
}
